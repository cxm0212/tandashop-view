mplate>\r\n\r\n<script>\r\nimport httpApi from '../../api/http';\r\nimport axios from 'axios';\r\n\r\nconst columns = [\r\n  {\r\n    title: \"序号\",\r\n    dataIndex: \"id\",\r\n    key: \"id\",\r\n    align: \"center\"\r\n  },\r\n  {\r\n    title: \"类别名称\",\r\n    dataIndex: \"categoryName\",\r\n    key: \"categoryName\",\r\n    align: \"center\",\r\n    width:'40%',\r\n  },\r\n  {\r\n    title: \"添加时间\",\r\n    dataIndex: \"createTime\",\r\n    key: \"createTime\",\r\n    align: \"center\"\r\n  },\r\n  {\r\n    title: \"修改时间\",\r\n    dataIndex: \"updateTime\",\r\n    key: \"updateTime\",\r\n    align: \"center\"\r\n  },\r\n  {\r\n    title: \"操作\",\r\n    key: \"action\",\r\n    scopedSlots: { customRender: \"action\" },\r\n    align: \"center\"\r\n  }\r\n];\r\n// const data = [\r\n//   {\r\n//     key: \"1\",\r\n//     id: 1,\r\n//     categoryName: \"自然文化\"\r\n//   },\r\n//   {\r\n//     key: \"2\",\r\n//     id: 2,\r\n//     categoryName: \"数学\"\r\n//   }\r\n// ];\r\nexport default {\r\n  data() {\r\n    return {\r\n      columns,\r\n      data:[],\r\n    };\r\n  },\r\n  created() {\r\n    this.form = this.$form.createForm(this);\r\n    this.getCategorys();\r\n  },\r\n  methods: {\r\n    addCategory() {\r\n      this.form.validateFields((error, params) => {\r\n        if (!error) {\r\n          console.log(params.categoryName);\r\n          axios.post(httpApi.addCategoryUrl,params).then((result) => {\r\n              console.log(result.data);\r\n              let datas = result.data;\r\n              if (datas.status == \"2000\") {\r\n                this.$message.error(datas.msg);\r\n              } else if (datas.status == \"2001\") {\r\n                this.$message.success(datas.msg);\r\n              }\r\n          }).catch((err) => {\r\n              console.log(err)\r\n          });      \r\n        }\r\n      });\r\n    },\r\n    getCategorys(categoryId){\r\n        axios.post(httpApi.getCategoryListUrl,{id:categoryId}).then((result) => {\r\n            console.log(result.data);\r\n            this.data = result.data;\r\n\r\n        }).catch((err) => {\r\n            console.log(err)\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/course"}]}